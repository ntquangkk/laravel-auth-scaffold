{
  "name": "triquang/laravel-auth-scaffold",
  "description": "Scaffold API & Web authentication for any Eloquent model with multi-auth support — monolith or modular Laravel.",
  "version": "2.0.0",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Nguyễn Trí Quang",
      "email": "ntquangkk@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^11.0 || ^12.0"
  },
  "autoload": {
    "psr-4": {
      "TriQuang\\LaravelAuthScaffold\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TriQuang\\LaravelAuthScaffold\\LaravelAuthScaffoldServiceProvider"
      ]
    }
  },
  "keywords": [
    "laravel",
    "authentication",
    "multi-auth",
    "auth-scaffold",
    "api-auth",
    "web-auth",
    "module",
    "modular",
    "generator",
    "ddd"
  ],
  "minimum-stability": "stable",
  "prefer-stable": true
}
